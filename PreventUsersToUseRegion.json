{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": "ec2:*",
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "ec2:Region": [
                        "us-west-2",
                        "us-west-1",
                        "us-east-1"
                    ]
                }
            }
        }
    ]